//programma pou na diavazei tous vathmous 50 foititwn kai n emfanizei ton meso oro tous kathws kai ton arithmo twn epitixontwn foititwn.
#include <stdio.h>
int main()
{
	int i, suc;
	float grd,sum;
	
	sum=suc=0;
	
	for(i=0; i<50; i++)
	{
		printf("Enter grade: ");
		scanf("%f", &grd);
		sum +=grd;
		if (grd >= 5)
		{
			suc++;
		}
	}
	printf("Avg: %f Suc: %d\n", sum/50,suc);
	return 0;
}
